{
  "name": "study-driving",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve && webpack-dev-server --open --mode development",
    "dev": "vue-cli-service serve && webpack-dev-server --open --mode development",
    "build": "vue-cli-service build",
    "build:development": "vue-cli-service build --mode production --env development",
    "build:test": "vue-cli-service build --mode production --env test",
    "build:production": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "getContainers": "node build/getContainers.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "core-js": "^2.6.5",
    "echarts": "^4.8.0",
    "resize-observer-polyfill": "^1.5.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.12.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "^3.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "express-http-proxy": "^1.6.0",
    "image-webpack-loader": "^5.0.0",
    "postcss-px2rem": "^0.3.0",
    "prettier": "^1.18.2",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "L": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": [
        0
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ],
  "vuePlugins": {
    "service": [
      "vue-cli-plugin/loadenv.js",
      "vue-cli-plugin/http-server.js"
    ]
  },
  "description": "```\r npm install\r ```",
  "main": "babel.config.js",
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
